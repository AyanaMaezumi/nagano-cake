会員一覧

<table class="table">
<thead class="table-light">
  <th>会員ID</th>
  <th>氏名</th>
  <th>メールアドレス</th>
  <th>ステータス</th>
</thead>
<tbody>
<% @customers.each  do |customers| %>

<tr>
<td><%= customers.id %></td>
<td><%= customers.last_name %><%= customers.first_name %></td>
<td><%= customers.email %></td>
<td>
<% if customers.is_deleted ==false %>有効
<% else %>退会
<% end %>
</td>
</tr>

<% end %>
</tbody>
</table>

<%= paginate @customers %>